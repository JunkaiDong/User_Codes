#!/bin/bash
#SBATCH -J mpitest
#SBATCH -o mpitest.out
#SBATCH -e mpitest.err
#SBATCH -p rocky
#SBATCH -n 8
#SBATCH -t 30
#SBATCH --mem-per-cpu=40000

# Load the required software modules, e.g.,
module load gcc/12.2.0-fasrc01 openmpi/4.1.4-fasrc01

# Run the executable
srun -n 8 --mpi=pmix ./mpitest.x
